{
  "name": "chonk",
  "version": "1.0.4",
  "description": "Check the size of a file and determine its chonkiness",
  "main": "index.js",
  "scripts": {
    "test:unit": "mocha test/unit/*.test.js",
    "test:integration": "mocha test/integration/*.test.js",
    "test": "nyc npm-run-all test:unit test:integration",
    "lint": "eslint-godaddy *.js **/*.js",
    "posttest": "npm run lint"
  },
  "bin": {
    "chonk": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/decompil3d/chonk.git"
  },
  "keywords": [
    "file",
    "size",
    "chonkiness",
    "chonk",
    "snark"
  ],
  "author": "Jonathan Keslin <github@jonathankeslin.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/decompil3d/chonk/issues"
  },
  "homepage": "https://github.com/decompil3d/chonk#readme",
  "dependencies": {
    "async": "^3.2.0",
    "chalk": "^4.0.0",
    "pretty-bytes": "^5.2.0",
    "yargs": "^16.0.0"
  },
  "devDependencies": {
    "@types/node": "^13.1.0",
    "@types/yargs": "^15.0.0",
    "assume": "^2.2.0",
    "concat-stream": "^2.0.0",
    "eslint": "^7.0.0",
    "eslint-config-godaddy": "^4.0.0",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-mocha": "^8.0.0",
    "mocha": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.0"
  }
}
