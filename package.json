{
  "name": "chonk",
  "version": "2.0.1",
  "description": "Check the size of a file and determine its chonkiness",
  "main": "index.js",
  "scripts": {
    "test:unit": "mocha test/unit/*.test.js",
    "test:integration": "mocha test/integration/*.test.js",
    "test": "nyc npm-run-all test:unit test:integration",
    "lint": "eslint-godaddy *.js **/*.js",
    "posttest": "npm run lint"
  },
  "bin": {
    "chonk": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/decompil3d/chonk.git"
  },
  "keywords": [
    "file",
    "size",
    "chonkiness",
    "chonk",
    "snark"
  ],
  "author": "Jonathan Keslin <github@jonathankeslin.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/decompil3d/chonk/issues"
  },
  "homepage": "https://github.com/decompil3d/chonk#readme",
  "dependencies": {
    "async": "^3.2.3",
    "chalk": "^4.1.2",
    "pretty-bytes": "^5.6.0",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/yargs": "^17.0.8",
    "assume": "^2.3.0",
    "concat-stream": "^2.0.0",
    "eslint": "^8.8.0",
    "eslint-config-godaddy": "^7.0.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-mocha": "^10.0.3",
    "mocha": "^10.0.0",
    "npm-run-all2": "^5.0.0",
    "nyc": "^15.1.0"
  }
}
